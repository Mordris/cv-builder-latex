# Specific Project/Dependency Files (Add as needed)
# =================================
# .nuxt/ # Nuxt.js build folder
# /public/hot # Laravel Mix hot reload file
# /storage/logs/*.log # Laravel logs
# /storage/framework/sessions/* # Laravel sessions
# /storage/framework/views/* # Laravel views cache
# /storage/framework/cache/* # Laravel cache
# /storage/app/public/* # Laravel storage link contents (usually gitignored)
# !/storage/app/public/.gitignore # Except for the .gitignore file itself